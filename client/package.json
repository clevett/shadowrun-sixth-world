{
  "author": "Cassie Levett",
  "name": "emeraldproductions-sixthworld",
  "description": "Shadowrun Sixth World Character Builder",
  "main": "index.js",
  "version": "0.0.1",
  "dependencies": {
    "@elastic/datemath": "^5.0.3",
    "@elastic/eui": "^37.1.4",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "babel-jest": "^27.2.0",
    "jest": "^27.2.0",
    "moment": "^2.29.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "recoil": "^0.4.1",
    "roll": "^1.3.0",
    "test-jest": "^1.0.1",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.2.1"
  },
  "scripts": {
    "react:start": "react-scripts start",
    "start": "npm-run-all --parallel react:start sass:watch",
    "build": "npm-run-all --parallel react:build sass:compile",
    "react:build": "react-scripts build",
    "react:test": "react-scripts test",
    "react:eject": "react-scripts eject",
    "sass:watch": "node-sass src/scss/index.scss src/index.css -w",
    "sass:compile": "node-sass src/scss/index.scss src/index.css --style compressed",
    "test": "jest --watch"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.15.6",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "@types/roll": "^1.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "jest-cli": "^26.6.1",
    "node-sass": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "react-ga": "^3.1.2",
    "typescript": "^4.4.3"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/mocks/fileMock.js",
      "\\.(css|less|scss|sass)$": "<rootDir>/mocks/fileMock.js"
    }
  }
}
